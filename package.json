{
  "name": "vite-plugin-macro",
  "description": "Brings macro capabilities to Vite based projects.",
  "keywords": [
    "macro",
    "vite",
    "plugin",
    "babel"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unbyte/vite-plugin-macro"
  },
  "author": "unbyte <i@shangyes.net>",
  "homepage": "https://github.com/unbyte/vite-plugin-macro/",
  "bugs": {
    "url": "https://github.com/unbyte/vite-plugin-macro/issues/new"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "version": "0.0.4",
  "scripts": {
    "dev": "node scripts/dev.mjs",
    "build": "node scripts/build.mjs",
    "lint": "eslint . --cache",
    "lint:nocache": "eslint .",
    "prepare": "is-ci || husky install"
  },
  "devDependencies": {
    "@types/mustache": "^4.1.2",
    "@types/node": "^16.3.2",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "chalk": "^4.1.2",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "shelljs": "^0.8.4",
    "tsup": "^4.12.5",
    "typescript": "^4.3.2",
    "vite": "^2.4.2"
  },
  "dependencies": {
    "@babel/generator": "^7.14.5",
    "@babel/parser": "^7.14.7",
    "@babel/template": "^7.14.5",
    "@babel/traverse": "^7.14.7",
    "@babel/types": "^7.14.5",
    "@types/babel__generator": "^7.6.3",
    "@types/babel__template": "^7.4.1",
    "@types/babel__traverse": "^7.14.2",
    "mustache": "^4.2.0"
  },
  "peerDependencies": {
    "vite": "^2"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
  "eslintIgnore": [
    "dist/*",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  }
}
